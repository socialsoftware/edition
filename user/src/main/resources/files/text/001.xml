                  <listBibl xml:id="Fr001.SRC.MS">
                     <!-- RELEVANT: The information for each manuscript -->
                     <msDesc xml:id="Fr001.SRC.MS.Fr001a">
                        <msIdentifier>
                           <!-- National Library -->
