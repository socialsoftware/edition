{
  "name": "visual",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "entry": "index.js",
  "config": {
    "docker": "http://localhost/ldod-mfes",
    "dev": "http://localhost:9000/ldod-mfes"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-dev": "vite --config vite.config.dev.js build",
    "prepreview": "vite --config vite.config.dev.js build",
    "preview": "vite --config vite.config.dev.js preview",
    "pack": "yarn build && ./scripts/pack.sh visual",
    "publish-dev": "yarn run pack && ./scripts/publish.sh visual visual.js @./dist/visual.tgz $npm_package_config_dev",
    "unpublish-dev": "./scripts/unpublish.sh visual $npm_package_config_dev",
    "publish": "yarn run pack && ./scripts/publish.sh visual visual.js @./dist/visual.tgz $npm_package_config_docker",
    "unpublish": "./scripts/unpublish.sh visual $npm_package_config_docker"
  },
  "devDependencies": {
    "vite": "^3.1.8"
  }
}
