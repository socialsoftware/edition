{
	"name": "new-mfe",
	"version": "1.0.0",
	"license": "MIT",
	"private": true,
	"type": "module",
	"entry": "index.js",
	"config": {
		"docker": "http://localhost:8080/ldod-mfes",
		"dev": "http://localhost:9000/ldod-mfes"
	},
	"scripts": {
		"test": "web-test-runner \"test/**/*.test.js\" --node-resolve",
		"dev": "vite",
		"build": "vite build",
		"postbuild": "node ./scripts/manifest.js",
		"pack": "yarn build && ./scripts/pack.sh new-mfe",
		"publish-dev": "yarn run pack && ./scripts/publish.sh new-mfe new-mfe.js @./dist/new-mfe.tgz $npm_package_config_dev",
		"unpublish-dev": "./scripts/unpublish.sh new-mfe $npm_package_config_dev",
		"publish": "yarn run pack && ./scripts/publish.sh new-mfe new-mfe.js @./dist/new-mfe.tgz $npm_package_config_docker",
		"unpublish": "./scripts/unpublish.sh new-mfe $npm_package_config_docker"
	},
	"devDependencies": {
		"@esm-bundle/chai": "^4.3.4-fix.0",
		"@open-wc/testing": "^3.1.8",
		"@rollup/plugin-terser": "latest",
		"@web/test-runner": "^0.16.1",
		"@web/test-runner-puppeteer": "^0.13.1",
		"lit": "^2.7.4",
		"shared": "file:../shell/shared",
		"vite": "latest"
	},
	"dependencies": {
		"lit-element-state_1.7.0": "npm:lit-element-state@1.7.0",
		"zustand_4.3.8": "npm:zustand@4.3.8"
	},
	"importmaps": {
		"@lit-bundle": "https://cdn.jsdelivr.net/gh/lit/dist@2.7.4/core/lit-core.min.js"
	}
}
