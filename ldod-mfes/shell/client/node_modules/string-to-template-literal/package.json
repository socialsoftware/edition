    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^6.2.2",
    "typescript": "^3.7.4"
