{
    "name": "about",
    "license": "MIT",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "entry": "index.js",
    "config": {
        "docker": "http://localhost/ldod-mfes",
        "dev": "http://localhost:9000/ldod-mfes"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build-dev": "vite --config vite.config.dev.js build",
        "prepreview": "vite --config vite.config.dev.js build",
        "preview": "vite --config vite.config.dev.js preview",
        "pack": "yarn build && ./scripts/pack.sh about",
        "publish-dev": "yarn run pack && ./scripts/publish.sh about about.js @./dist/about.tgz $npm_package_config_dev"
    },
    "devDependencies": {
        "vite": "^3.0.7"
    }
}