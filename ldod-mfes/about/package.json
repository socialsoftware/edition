{
  "name": "about",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "entry": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-dev": "vite --config vite.config.dev.js build",
