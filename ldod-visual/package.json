    "dependencies": {
        "axios": "^0.19.0",
        "enzyme": "^3.10.0",
        "hashmap": "^2.3.0",
        "html-react-parser": "^0.6.4",
        "moment": "^2.24.0",
        "npm": "^6.11.1",
        "qs": "^6.8.0",
        "rc-slider": "^8.6.13",
        "rc-tooltip": "^3.7.3",
        "react": "^16.9.0",
        "react-animate-on-change": "^2.2.0",
        "react-bootstrap": "^0.32.4",
        "react-d3-cloud": "^0.7.0",
        "react-dom": "^16.9.0",
        "react-favicon": "0.0.14",
        "react-helmet": "^5.2.1",
        "react-html-parser": "^2.0.2",
        "react-idle-timer": "^4.2.8",
        "react-jss": "^8.6.1",
        "react-modal": "^3.9.1",
        "react-player": "^1.12.0",
        "react-rangeslider": "^2.2.0",
        "react-router-dom": "^5.0.1",
        "react-scripts": "^3.1.1",
        "react-scroll": "^1.7.13",
        "react-slider": "^0.11.2",
        "react-table": "^6.10.0",
        "redux-persist": "^5.10.0",
        "sleep-async": "^1.0.5",
        "start": "^5.1.0",
        "typescript": "^3.5.3",
        "vis": "^4.21.0",
        "vis-react": "^0.4.5",
        "yarn": "^1.17.3"
    },
    "config": {
        "docker": "http://localhost/ldod-mfes/",
        "dev": "http://localhost:9000/ldod-mfes"
    },
    "scripts": {
        "start": "export NODE_OPTIONS=--openssl-legacy-provider && react-app-rewired start",
        "build": "export NODE_OPTIONS=--openssl-legacy-provider && react-app-rewired build",
        "test": "react-app-rewired test --env=jsdom",
        "eject": "react-scripts eject",
        "pack": "npm run build && rm -rf dist && mkdir dist &&  tar -czvf dist/ldod-visual.tgz -C ./build ./",
        "publish-dev": "npm run pack && curl -F id=ldod-visual -F file=@./dist/ldod-visual.tgz $npm_package_config_dev/publish",
        "publish": "npm run pack && curl -F id=ldod-visual -F file=@./dist/ldod-visual.tgz $npm_package_config_docker/publish",
        "unpublish-dev": "curl -d id=ldod-visual $npm_package_config_dev/unpublish",
        "unpublish": "curl -d id=ldod-visual $npm_package_config_docker/unpublish"
    },
    "devDependencies": {
        "babel-plugin-import": "^1.12.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "customize-cra": "^0.2.14",
        "react-app-rewire-less": "^2.1.3",
        "react-app-rewired": "^2.1.3",
        "react-redux": "^5.0.7",
        "redux": "^4.0.4"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}